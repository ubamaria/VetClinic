
@{
    ViewData["Title"] = "Личный кабинет";
}
<div class="text-center text-info">
    <h1>@ViewData["Title"]</h1>
</div>

<p>
    Имя: @ViewBag.Client.ClientFIO<br>
    Логин: @ViewBag.Client.Login<br>
    Почта: @ViewBag.Client.Email<br>
    Номер телефона: @ViewBag.Client.Phone<br>
    Пароль: @ViewBag.Client.Password
    Имя: @ViewBag.Pet.PetName<br>
    Вид: @ViewBag.Pet.Kind<br>
    Порода: @ViewBag.Pet.Breed<br>
    Возраст: @ViewBag.Pet.Age<br>
    Пол: @ViewBag.Pet.Gender
</p>

